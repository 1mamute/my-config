{
  "python.languageServer": "Pylance",
  "editor.tabSize": 2,
  "terminal.integrated.cursorStyle": "line",
  "editor.minimap.enabled": false,
  "git.autofetch": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.fontFamily": "FiraCode Nerd Font",
  "editor.formatOnSave": true,
  "explorer.compactFolders": false,
  "editor.mouseWheelZoom": true,
  "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.minimap.maxColumn": 100,
  "editor.minimap.showSlider": "always",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.highlightModifiedTabs": true,
  "search.smartCase": true,
  "terminal.external.linuxExec": "alacritty",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "python.formatting.provider": "yapf",
  "window.titleBarStyle": "custom",
  "window.enableMenuBarMnemonics": false,
  "window.customMenuBarAltFocus": false,
  "auto-rename-tag.activationOnLanguage": [
    "html",
    "xml",
    "php",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.fontSize": 16,
  "files.associations": {
    "*.tf": "terraform"
  },
  "eslint.format.enable": true,
  "[javascript,javascriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
  },
  "[typescript,typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.autoImports": true,
  "typescript.suggest.autoImports": true,
  "yaml.schemaStore.enable": false,
  "editor.linkedEditing": true,
  "aws.telemetry": false,
  "redhat.telemetry.enabled": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "terraform.languageServer": {
    "pathToBinary": "/usr/bin/terraform-ls",
    "external": true,
    "args": [
      "serve"
    ]
  },
  "terraform-ls.experimentalFeatures": {
    "validateOnSave": true
  },
  "gopls": {
    "build.experimentalWorkspaceModule": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "jupyter.askForKernelRestart": false,
  "git.allowForcePush": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "discord.removeRemoteRepository": true,
  "discord.lowerDetailsEditing": "Repository: {workspace}",
  "discord.lowerDetailsNoWorkspaceFound": "No repository",
  "discord.swapBigAndSmallImage": true,
  "yaml.schemas": {},
  "editor.tokenColorCustomizations": {
    "[Nord]": {
      "comments": "#909aad"
    }
  },
  "workbench.colorTheme": "Nord",
  "aws.profile": "profile:1mamute",
  "vs-kubernetes": {
    "vs-kubernetes.knownKubeconfigs": [
      "/home/gb/repos/1mamute/1mamute-aws/terraform/kubeconfig_test-eks-1mamute"
    ],
    "vs-kubernetes.kubeconfig": "/home/gb/repos/1mamute/1mamute-aws/terraform/kubeconfig_test-eks-1mamute"
  },
  "indentRainbow.colors": [
    "rgba(16,16,16,0.1)",
    "rgba(16,16,16,0.2)",
    "rgba(16,16,16,0.3)",
    "rgba(16,16,16,0.4)",
    "rgba(16,16,16,0.5)",
    "rgba(16,16,16,0.6)",
    "rgba(16,16,16,0.7)",
    "rgba(16,16,16,0.8)",
    "rgba(16,16,16,0.9)",
    "rgba(16,16,16,1.0)"
  ],
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "editor.bracketPairColorization.enabled": true,
  "diffEditor.codeLens": true,
  "gitlens.codeLens.includeSingleLineSymbols": true,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": [
    "--max-line-length=120"
  ],
  "editor.codeLens": false,
  "json.schemas": []
}